@model ISD.ViewModels.Home.AboutViewModel
@{
    ViewBag.Title = Model.pagesData == null ? "" : Model.pagesData.PageTitle;
}


<main class="pb-0">

    <div class="container py-4">

        <section id="products" class="section feature-box wow fadeIn  mt-4 mb-3 pb-3 pt-5 " data-wow-delay="0.3s" style="animation-name: none; visibility: visible;">
         
            <!-- Section description -->
            <div class="section-description lead text-justify">
                @Html.Raw(@Model.pagesData == null ? "" : Model.pagesData.PageContent)
                
            </div>
          
        </section>




       
    </div>

</main>
