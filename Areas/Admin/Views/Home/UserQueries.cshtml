@model IEnumerable<ISD.Models.CustomerQuery>

@{
    ViewBag.Title = "UserQueries";
}

<h2>User Queries</h2>

<table class=" table table-striped">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Query</th>
    </tr>
    <tbody>
        @foreach (var dd in Model)
        {
            <tr>
                <td>@dd.Name</td>
                <td>@dd.Email</td>
                <td>@dd.PhoneNo</td>
                <td>@dd.Message</td>
            </tr>
        }
    </tbody>
</table>



